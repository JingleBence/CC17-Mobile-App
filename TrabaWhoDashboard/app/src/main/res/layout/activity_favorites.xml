<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="#D5EFE3"
    android:id="@+id/drawer_layout"
    tools:context=".Favorites">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:layout_width="178dp"
                android:layout_height="55dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="9dp"
                android:layout_marginEnd="172dp"
                android:layout_marginBottom="6dp"
                android:autoSizeTextType="uniform"
                android:text="Likes/Favorites"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/messagesHome"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0">

            </TextView>

            <ImageView
                android:id="@+id/favoritesHome"
                android:layout_width="62dp"
                android:layout_height="51dp"
                android:layout_marginStart="148dp"
                android:layout_marginTop="9dp"
                android:layout_marginEnd="5dp"
                android:background="?attr/selectableItemBackgroundBorderless"
                android:clickable="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/imageView"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/home" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout2"
            android:layout_width="match_parent"
            android:layout_height="775dp"
            android:layout_marginTop="17dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.538"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout">

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView9"
                android:layout_width="390dp"
                android:layout_height="110dp"
                app:cardBackgroundColor="#85a3c5"
                android:layout_marginStart="10dp"
                android:layout_marginTop="68dp"
                android:layout_marginEnd="11dp"
                android:layout_marginBottom="38dp"
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                app:cardCornerRadius="15dp"
                app:layout_constraintBottom_toTopOf="@+id/cardView11"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:layout_width="82dp"
                    android:layout_height="80dp"
                    android:layout_marginTop="15dp"
                    app:srcCompat="@drawable/profile">

                </ImageView>


                <TextView
                    android:layout_width="149dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="75dp"
                    android:layout_marginTop="15dp"
                    android:autoSizeTextType="uniform"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:text="Technician"
                    >
                </TextView>

                <TextView
                    android:layout_width="149dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="75dp"
                    android:layout_marginTop="50dp"
                    android:autoSizeTextType="uniform"
                    android:textColor="@color/black"
                    android:text="Appliance Repair"
                    >
                </TextView>

                <ImageView
                    android:layout_width="65dp"
                    android:layout_height="53dp"
                    android:layout_marginLeft="310dp"
                    android:layout_marginTop="30dp"
                    app:srcCompat="@drawable/heart">

                </ImageView>



            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView11"
                android:layout_width="390dp"
                android:layout_height="110dp"
                app:cardBackgroundColor="#85a3c5"
                android:layout_marginStart="28dp"
                android:layout_marginTop="36dp"
                android:layout_marginEnd="18dp"
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                app:cardCornerRadius="15dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.72"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cardView9">

                <TextView
                    android:layout_width="149dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="75dp"
                    android:layout_marginTop="15dp"
                    android:autoSizeTextType="uniform"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:text="Technician"
                    >
                </TextView>

                <TextView
                    android:layout_width="149dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="75dp"
                    android:layout_marginTop="50dp"
                    android:autoSizeTextType="uniform"
                    android:textColor="@color/black"
                    android:text="Welding Repair"
                    >
                </TextView>

                <ImageView
                    android:layout_width="82dp"
                    android:layout_height="80dp"
                    android:layout_marginTop="15dp"
                    app:srcCompat="@drawable/profile">

                </ImageView>

                <ImageView
                    android:layout_width="65dp"
                    android:layout_height="53dp"
                    android:layout_marginLeft="310dp"
                    android:layout_marginTop="30dp"
                    app:srcCompat="@drawable/heart">

                </ImageView>


            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/textView16"
                android:layout_width="118dp"
                android:layout_height="42dp"
                android:layout_marginTop="34dp"
                android:layout_marginEnd="276dp"
                android:layout_marginBottom="22dp"
                android:autoSizeTextType="uniform"
                android:text="Saved"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/cardView10"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cardView11"
                app:layout_constraintVertical_bias="0.0">

            </TextView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView10"
                android:layout_width="390dp"
                android:layout_height="120dp"
                app:cardBackgroundColor="#85a3c5"
                android:layout_marginStart="13dp"
                android:layout_marginTop="22dp"
                android:layout_marginEnd="8dp"
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                app:cardCornerRadius="15dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView16">

                <ImageView
                    android:layout_width="82dp"
                    android:layout_height="80dp"
                    android:layout_marginTop="20dp"
                    app:srcCompat="@drawable/profile">

                </ImageView>

                <TextView
                    android:layout_width="149dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="75dp"
                    android:layout_marginTop="15dp"
                    android:autoSizeTextType="uniform"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:text="Technician"
                    >
                </TextView>

                <TextView
                    android:layout_width="149dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="75dp"
                    android:layout_marginTop="50dp"
                    android:autoSizeTextType="uniform"
                    android:textColor="@color/black"
                    android:text="Electrical Repair"
                    >
                </TextView>

                <ImageView
                    android:layout_width="65dp"
                    android:layout_height="53dp"
                    android:layout_marginLeft="310dp"
                    android:layout_marginTop="30dp"
                    app:srcCompat="@drawable/heart">


                </ImageView>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView10"
                android:layout_width="390dp"
                android:layout_height="120dp"
                android:layout_marginStart="13dp"
                android:layout_marginTop="168dp"
                android:layout_marginEnd="8dp"
                android:clickable="true"
                android:foreground="?android:attr/selectableItemBackground"
                app:cardBackgroundColor="#85a3c5"
                app:cardCornerRadius="15dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView16">

                <TextView
                    android:layout_width="149dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="75dp"
                    android:layout_marginTop="15dp"
                    android:autoSizeTextType="uniform"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:text="Technician"
                    >
                </TextView>

                <TextView
                    android:layout_width="149dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="75dp"
                    android:layout_marginTop="50dp"
                    android:autoSizeTextType="uniform"
                    android:textColor="@color/black"
                    android:text="Appliance Repair"
                    >
                </TextView>

                <ImageView
                    android:layout_width="82dp"
                    android:layout_height="80dp"
                    android:layout_marginTop="20dp"
                    app:srcCompat="@drawable/profile">

                </ImageView>

                <ImageView
                    android:layout_width="65dp"
                    android:layout_height="53dp"
                    android:layout_marginLeft="310dp"
                    android:layout_marginTop="30dp"
                    app:srcCompat="@drawable/heart">

                </ImageView>


            </androidx.cardview.widget.CardView>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>